<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <StartupObject></StartupObject>
    <ApplicationIcon>bin\Release\net6.0-windows\ECIconfigurator.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Device.contrast-black.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="bin\Release\net6.0-windows\ECIconfigurator.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="System.IO.Ports" Version="7.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Device.contrast-black.ico" />
  </ItemGroup>

</Project>
